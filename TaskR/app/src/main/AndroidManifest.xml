<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.bignerdranch.taskr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Searching"></activity>
        <activity android:name=".FriendList" />
        <activity android:name=".NonFriendProfile" />
        <activity android:name=".Profile" />
        <activity android:name=".TaskView" />
        <activity android:name=".CreatingTask" />
        <activity android:name=".DirectMessageChat" />
        <activity android:name=".DirectMessagesList" />
        <activity android:name=".Calendar" />
        <activity
            android:name=".Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>